generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch", "fullTextIndex"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Images{
  id String @id @default(uuid())
  name String @db.Text
  url String
  public_id String?
  createdAt DateTime @default(now())
  width Int
  height Int
  @@index([name])
}